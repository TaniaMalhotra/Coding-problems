#include<iostream>
using namespace std;
struct node{
	int data;
	node * next;
};
int main(){
	//creating linked list
	node * head = NULL;
	int l,value;
	cout<<"Enter length of inkedlist"<<endl;
	cin>>l;
	for(int i = 0; i<l; i++){
	cout<<"Enter the value of data"<<endl;
	cin>>value;
	node * temp = new node;
	temp->data = value;
	temp->next = head;
	head = temp;
    }
}
