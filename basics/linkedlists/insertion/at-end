#include<iostream>
using namespace std;
struct node{
	int data;
	node * next;
};
int insert(){
	
}
int main(){
	//insert at end
	node * head = NULL;
	
	int l,value;
	cout<<"Enter length of linkedlist"<<endl;
	cin>>l;
	for(int i = 0; i<l; i++){
	cout<<"Enter the value of data"<<endl;
	cin>>value;
	node * temp = new node;
	temp->data = value;
	temp->next = NULL;
	if(head == NULL){
	 head = temp;
    }
    else{
    	node * temp1 = head;
    	while(temp1->next != NULL){
    		temp1 = temp1->next;
		}
		temp1->next = temp;
	}
}
    //display linkedlist
    if(head!=NULL){
    	node * temp = head;
    	while(temp != NULL){
    		cout<<temp->data;
    		temp = temp->next;
		}
	}
	else{
		cout<<"empty";
	}
}

