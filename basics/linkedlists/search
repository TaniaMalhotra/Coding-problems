#include<iostream>
using namespace std;
struct node{
	int data;
	node * next;
};
int insert(){

}
int main(){
	//creating linked list
	//insert at head
	node * head = NULL; //head naam ka pointer with null value dabba white hissa greean

	int l,value;
	cout<<"Enter length of inkedlist"<<endl;
	cin>>l;
	for(int i = 0; i<l; i++){
	cout<<"Enter the value of data"<<endl;
	cin>>value;
	node * temp = new node;
	temp->data = value;
	temp->next = head;
	head = temp;
    }
    //display linkedlist
    if(head!=NULL){
    	node * temp = head;
    	while(temp != NULL){
    		cout<<temp->data;
    		temp = temp->next;
		}
	}
	else{
		cout<<"empty";
	}
}
